[out:json][timeout:25000];
area["name:en"="Switzerland"]->.ch;
(
nwr(area.ch)["owner:type"="cooperative"]["building"~"^(residental|apartments)$"];
nwr(area.ch)["owner:type"="cooperative"]["landuse"="residential"];
nwr(area.ch)["operator:type"="cooperative"]["building"~"^(residental|apartments)$"];
nwr(area.ch)["operator:type"="cooperative"]["landuse"="residential"];
);
(._;>;);
out body;
