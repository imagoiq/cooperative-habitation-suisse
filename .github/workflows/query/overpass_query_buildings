[out:json][timeout:25000];
area["name:en"="Switzerland"]->.ch;
(
nwr(area.ch)["owner:type"="cooperative"]["building"~"^(residental|apartments|dormitory|sheltered_housing)$"];
nwr(area.ch)["operator:type"="cooperative"]["building"~"^(residental|apartments|dormitory|sheltered_housing)$"];
);
out geom;
